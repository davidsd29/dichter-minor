{
  "name": "dichter-api-minor",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node server.js",
    "predev": "npm run build",
    "dev": "nodemon -e js,json,html,eta start server.js",
    "prebuild": "rimraf ./public",
    "build": "npm-run-all build:*",
    "build:css": "node build/build-css.js",
    "build:js": "node build/build-js.js",
    "build:assets": "node build/build-assets.js",
    "watch": "run-p watch:*",
    "watch:css": "chokidar 'src/scss/**/*.scss' --command 'npm run build:css'",
    "watch:assets": "chokidar 'src/assets/**/*.*' --command 'npm run build:assets'",
    "watch:js": "chokidar 'src/js/**/*.js' --command 'npm run build:js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^4.2.0",
    "@fastify/bearer-auth": "^9.0.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/static": "^6.10.0",
    "@fastify/view": "^7.4.1",
    "dotenv": "^16.0.3",
    "eta": "^2.0.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.0.6",
    "express-rate-limit": "^6.7.0",
    "fastify": "^4.15.0",
    "node-appwrite": "^8.2.0",
    "node-fetch": "^2.6.9",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^6.0.0",
    "gulp-line-ending-corrector": "^1.0.3",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.61.0"
  }
}
